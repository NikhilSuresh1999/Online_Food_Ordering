<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="ISO-8859-1">
<title>Register here</title>
</head>
<body>
<h1>Register here</h1>


<br>
<div class="register">
<form method="post" action="reg">

<label> <b> Username</b></label>
<input type="text" name="uname" id="textbox" placeholder="enter the username"/>
<br>
<br>
<label> <b> First Name</b></label>
<input type="text" name="fname" id="textbox" placeholder="enter the firstname"/>
<br>
<br>
<label> <b> Last Name</b></label>
<input type="text" name="lname" id="textbox" placeholder="enter the lastname"/>
<br>
<br>
<label> <b> Email</b></label>
<input type="text" name="email" id="textbox" placeholder="enter the email"/>
<br>
<br>
<label> <b> phone number</b></label>
<input type="text" name="phone" id="textbox" placeholder="enter the phone number"/>
<br>
<br>
<label> <b> Password</b></label>
<input type="text" name="password" id="textbox" placeholder="enter the password"/>
<br>
<br>
<label> <b> Address</b></label>
<input type="text" name="address" id="textbox" placeholder="enter the address"/>
<br>
<br>
<label> <b> Status</b></label>
<input type="number" name="status" id="textbox" placeholder="enter the status 1 or 0"/>
<br>
<br>
<input type="submit" value="register"/>
<br>



</form>

</div>


</body>
</html>